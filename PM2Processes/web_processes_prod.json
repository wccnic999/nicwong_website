[{
  "name"        : "mtgamer_esports_web",
  "script"      : "keystone.js",
  "args"        : "[]",
  "ignoreWatch" : ["node_modules"],
  "watch"       : "true",
  "exec_mode"  : "cluster_mode",
  "instances"  : "max",  
  "log_date_format": "YYYY-MM-DD HH:mm:ss:SSS Z",
  "error_file" : "/var/log/pm2/mtgamer_esports-err.log",
  "out_file"   : "/var/log/pm2/mtgamer_esports-out.log",
  "env": {
      "NODE_ENV":"production",
      "APP_ENV":"production",
      "CLIENT_PLATFORM": "android",
      "KEYSTONE_MODE":"api"
  }
}]
